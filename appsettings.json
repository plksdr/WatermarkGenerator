{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Error": "Error"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "File": {
      "LogLevel": {
        "Default": "Information",
        "Error": "Error"
      },
      "Path": "logs",
      "FileName": "log-",
      "FileExtension": "txt",
      "FileSizeLimitBytes": 10485760,
      "RetainedFileCountLimit": 5,
      "FlushInterval": "00:00:30",
      "Append": true
    }
  },
  "Watermark": {
    "Big": {
      "FontSize": 30,
      "OffsetY": 16,
      "PaddingX": 12,
      "PaddingY": 10,
      "FilePath": "font/Poppins-Regular.ttf"
    },
    "Small": {
      "FontSize": 10,
      "OffsetY": 4,
      "PaddingX": 3,
      "PaddingY": 2,
      "FilePath": "font/Poppins-Regular.ttf"
    },
    "Cutoff": 512
  },
  "Resize": {
    "Width": 128,
    "Height": 128
  },
  "AllowedHosts": "*",
  "Port": 5050,
  "Urls": "http://localhost:5050"
}
