{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Error": "Error"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "File": {
      "LogLevel": {
        "Default": "Information",
        "Error": "Error"
      },
      "Path": "logs",
      "FileName": "log-",
      "FileExtension": "txt",
      "FileSizeLimitBytes": 10485760,
      "RetainedFileCountLimit": 5,
      "FlushInterval": "00:00:30",
      "Append": true
    }
  },
  "Watermark": {
    "fontSize": 40,
    "paddingX": 1,
    "paddingY": 1,
    "filePath": "font/SpaceMono-Regular.ttf"
  },
  "Resize": {
    "Width": 128,
    "Height": 128
  },
  "AllowedHosts": "*",
  "Port": 5050,
  "Urls": "http://localhost:5050"
}
